{
    "name": "alpharaver/laravel7-materialize-css",
    "description": "Add Materialize CSS Framework to Laravel 7",
    "keywords": ["Materialize", "CSS", "Material Design", "laravel", "CSS Framework"],
    "type": "package",
    "license": "MIT",
    "homepage": "http://alpharaver.com",
    "require": {
        "php": ">=7.2.5",
        "illuminate/support": "7.*"
    },
    "authors": [
        {
            "homepage": "http://www/alpharaver.com",
            "name": "Alpha Raver",
            "email": "iamalpharaver@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Alpharaver\\LaravelMaterializeCSS\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alpharaver\\LaravelMaterializeCSS\\MaterializeCSSServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
